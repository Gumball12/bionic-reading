{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "sandbox#dev": {
      "dependsOn": ["text-vide#build"],
      "outputs": []
    },
    "preview": {
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["text-vide#build"],
      "cache": false
    },
    "benchmark": {
      "outputs": []
    },
    "release": {
      "cache": false
    }
  }
}
