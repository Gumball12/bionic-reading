{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "preview": {
      "dependsOn": ["build"]
    },
    "test": {
      "cache": false
    },
    "release": {
      "cache": false
    }
  }
}
