{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "sourceMap": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}